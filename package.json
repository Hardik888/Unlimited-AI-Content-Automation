{
  "name": "strapi-content-automation",
  "version": "1.0.0",
  "description": "Backend for strapi content automation",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": " npm run clean && tsc",
    "build:local": " npm run clean && tsc && npx cpy local.json dist/config",
    "start": "node dist/index.js",
    "deploy": "export AWS_PROFILE=dharXhardik && npm run build && serverless deploy",
    "offline": "npm run build && serverless offline",
    "offline:local": "npm run build:local && serverless offline",
    "test": "jest",
    "db:migrate": "ts-node src/db/migrate.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.859.0",
    "@aws-sdk/client-sqs": "^3.758.0",
    "@aws-sdk/util-dynamodb": "^3.859.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "dotenv": "^16.3.1",
    "install": "^0.13.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.12.0",
    "plugin": "^0.3.3",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/node": "^20.17.23",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.5.0",
    "serverless": "^3.40.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^14.4.0",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  }
}
